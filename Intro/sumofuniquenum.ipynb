   "source": [
    "def sumofunique(ls):\n",
    "    x=set()\n",
    "    for i in ls:\n",
    "        x.add(i)\n",
    "        print(x)\n",
    "\n",
    "    s=0\n",
    "    for i in x:\n",
    "        s=s+i\n",
    "        print(s)\n",
    "    \n",
    "\n",
    "ls=[1,2,2,4,5,3,6,3,4,5,6,1,2]\n",
    "sumofunique(ls)"
   ]
